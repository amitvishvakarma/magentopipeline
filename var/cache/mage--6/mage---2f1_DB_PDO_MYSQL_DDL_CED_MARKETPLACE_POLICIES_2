"CREATE TABLE `ced_marketplace_policies` (\n  `id` smallint(5) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Policy Id',\n  `marketplace_id` tinyint(3) unsigned NOT NULL COMMENT 'Associated Maketplace Id',\n  `name` varchar(100) DEFAULT NULL COMMENT 'Policy Name',\n  `type` varchar(50) NOT NULL COMMENT 'Policy Type',\n  `policy_data` text DEFAULT NULL COMMENT 'Policy Data',\n  `is_default` smallint(6) NOT NULL DEFAULT 0 COMMENT 'Is Policy Default',\n  `valid` tinyint(1) NOT NULL DEFAULT 0 COMMENT 'Valid',\n  `created_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Created At',\n  `updated_at` timestamp NOT NULL DEFAULT current_timestamp() ON UPDATE current_timestamp() COMMENT 'Updated At',\n  PRIMARY KEY (`id`)\n) ENGINE=InnoDB AUTO_INCREMENT=11 DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci COMMENT='Marketplace Policies'"