"CREATE TABLE `ced_attribute_mapping` (\n  `id` smallint(5) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Attribute Id',\n  `marketplace_id` tinyint(3) unsigned NOT NULL COMMENT 'Associated Maketplace Id',\n  `category_id` smallint(5) unsigned NOT NULL COMMENT 'Associated Category Id',\n  `marketplace_attribute_code` varchar(100) NOT NULL COMMENT 'Marketplace Attribute Code',\n  `marketplace_attribute_name` varchar(100) DEFAULT NULL COMMENT 'Marketplace Attribute Name',\n  `magento_attribute_code` varchar(255) DEFAULT NULL COMMENT 'Magento Attribute Code',\n  `default_custom_value` varchar(255) DEFAULT NULL COMMENT 'Default Custom Value',\n  `default_recommended_value` varchar(255) DEFAULT NULL COMMENT 'Default Recommended Value',\n  `type` varchar(50) NOT NULL COMMENT 'Type of Attribute',\n  `mandatory` tinyint(1) NOT NULL DEFAULT 0 COMMENT 'Mandatory',\n  `category_dependent` tinyint(1) NOT NULL COMMENT 'Category Dependent',\n  `is_variant` tinyint(1) NOT NULL COMMENT 'Is Variant',\n  `option_values` text DEFAULT NULL COMMENT 'Marketplace Option Values',\n  `option_value_mapping` text DEFAULT NULL COMMENT 'Marketplace\/Magento Option Mapping',\n  `additional_data` text DEFAULT NULL COMMENT 'Additional Data',\n  `is_changed` tinyint(1) DEFAULT 0 COMMENT 'Is Changed',\n  PRIMARY KEY (`id`),\n  KEY `CED_ATTRIBUTE_MAPPING_MARKETPLACE_ID_CED_MARKETPLACES_ID` (`marketplace_id`),\n  CONSTRAINT `CED_ATTRIBUTE_MAPPING_MARKETPLACE_ID_CED_MARKETPLACES_ID` FOREIGN KEY (`marketplace_id`) REFERENCES `ced_marketplaces` (`id`) ON DELETE CASCADE\n) ENGINE=InnoDB AUTO_INCREMENT=118 DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci COMMENT='Attribute Mapping'"