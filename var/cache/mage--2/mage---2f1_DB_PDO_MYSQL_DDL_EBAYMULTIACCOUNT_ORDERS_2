"CREATE TABLE `ebaymultiaccount_orders` (\n  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',\n  `ebaymultiaccount_order_id` varchar(50) NOT NULL DEFAULT '' COMMENT 'eBay Order Id',\n  `extended_order_id` varchar(50) NOT NULL DEFAULT '' COMMENT 'eBay Extended Order Id',\n  `magento_order_id` varchar(50) NOT NULL DEFAULT '' COMMENT 'Magento Order Id',\n  `order_place_date` datetime NOT NULL COMMENT 'Order Place Date',\n  `status` varchar(100) DEFAULT '' COMMENT 'eBay Order Status',\n  `order_data` text DEFAULT NULL COMMENT 'Order Data',\n  `shipment_data` text DEFAULT NULL COMMENT 'Order ShipmentPolicies Data',\n  `ebaymultiaccount_record_no` varchar(255) DEFAULT '' COMMENT 'eBay Record Number',\n  `magento_id` int(10) unsigned DEFAULT 0 COMMENT 'Magento Order Id',\n  `failed_order_reason` text DEFAULT NULL COMMENT 'Failed Order Reason',\n  `account_id` int(11) DEFAULT NULL COMMENT 'Account Id',\n  `user_id` varchar(50) NOT NULL DEFAULT '' COMMENT 'eBay User Id',\n  PRIMARY KEY (`id`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci COMMENT='eBay Orders'"