"CREATE TABLE `ced_product_linking` (\n  `id` smallint(5) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',\n  `marketplace_id` tinyint(3) unsigned NOT NULL COMMENT 'Marketplace ID',\n  `account_id` smallint(5) unsigned NOT NULL COMMENT 'Account ID',\n  `marketplace_sku` varchar(100) NOT NULL COMMENT 'Marketplace SKU',\n  `magento_sku` varchar(100) NOT NULL COMMENT 'Magento SKU',\n  `linking_status` varchar(32) DEFAULT NULL COMMENT 'Linking Status',\n  `additional_data` text DEFAULT NULL COMMENT 'Additional Data',\n  PRIMARY KEY (`id`),\n  KEY `CED_PRODUCT_LINKING_MARKETPLACE_ID_CED_MARKETPLACES_ID` (`marketplace_id`),\n  KEY `CED_PRODUCT_LINKING_ACCOUNT_ID_CED_MARKETPLACE_ACCOUNT_ID` (`account_id`),\n  CONSTRAINT `CED_PRODUCT_LINKING_ACCOUNT_ID_CED_MARKETPLACE_ACCOUNT_ID` FOREIGN KEY (`account_id`) REFERENCES `ced_marketplace_account` (`id`) ON DELETE CASCADE,\n  CONSTRAINT `CED_PRODUCT_LINKING_MARKETPLACE_ID_CED_MARKETPLACES_ID` FOREIGN KEY (`marketplace_id`) REFERENCES `ced_marketplaces` (`id`) ON DELETE CASCADE\n) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci COMMENT='Marketplace Product Linking'"