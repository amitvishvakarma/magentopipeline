"CREATE TABLE `ced_activities_log` (\n  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',\n  `product_id` varchar(255) DEFAULT NULL COMMENT 'Product Id',\n  `product_sku` varchar(64) DEFAULT NULL COMMENT 'Product Sku',\n  `marketplace_order_id` varchar(255) DEFAULT NULL COMMENT 'Marketplace Order ID',\n  `magento_increment_id` varchar(32) DEFAULT NULL COMMENT 'Increment ID',\n  `account_id` tinyint(3) unsigned NOT NULL COMMENT 'Account ID',\n  `section` varchar(20) NOT NULL COMMENT 'Section',\n  `severity` int(10) unsigned NOT NULL COMMENT 'Severity',\n  `type` varchar(50) NOT NULL COMMENT 'Type',\n  `mode` varchar(30) NOT NULL COMMENT 'Mode',\n  `message` text DEFAULT NULL COMMENT 'Message',\n  `additional_data` text DEFAULT NULL COMMENT 'Additional Data',\n  `logged_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Created At',\n  PRIMARY KEY (`id`)\n) ENGINE=InnoDB AUTO_INCREMENT=3077 DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci COMMENT='Activity Log'"