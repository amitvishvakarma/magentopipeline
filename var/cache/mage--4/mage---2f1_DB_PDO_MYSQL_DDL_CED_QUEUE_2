"CREATE TABLE `ced_queue` (\n  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Queue ID',\n  `name` varchar(255) DEFAULT NULL COMMENT 'Queue Name',\n  `updated_at` timestamp NOT NULL DEFAULT current_timestamp() ON UPDATE current_timestamp() COMMENT 'Updated At',\n  `status` smallint(5) unsigned NOT NULL COMMENT 'Status in particular queue',\n  `number_of_trials` smallint(5) unsigned NOT NULL DEFAULT 0 COMMENT 'Number of trials to processed failed queue processing',\n  `priority` smallint(5) unsigned NOT NULL DEFAULT 0 COMMENT 'Priority in particular queue',\n  `account_id` tinyint(3) unsigned NOT NULL COMMENT 'Account ID',\n  `marketplace_id` tinyint(3) unsigned NOT NULL COMMENT 'Marketplace Id',\n  `template_id` smallint(5) unsigned DEFAULT NULL COMMENT 'Template Id',\n  `feed_id` varchar(255) DEFAULT NULL COMMENT 'Feed Id',\n  `process_type` varchar(255) DEFAULT NULL COMMENT 'Process Type',\n  `body` longtext DEFAULT NULL COMMENT 'Message body',\n  `description` text DEFAULT NULL COMMENT 'Description',\n  `failure_reason` text DEFAULT NULL COMMENT 'Failure Reason',\n  PRIMARY KEY (`id`)\n) ENGINE=InnoDB AUTO_INCREMENT=31 DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci COMMENT='Table storing unique queues'"