"CREATE TABLE `ced_marketplace_account` (\n  `id` smallint(5) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',\n  `marketplace_id` tinyint(3) unsigned NOT NULL COMMENT 'Marketplace ID',\n  `title` varchar(100) NOT NULL COMMENT 'Title',\n  `country_id` varchar(30) NOT NULL COMMENT 'Country ID',\n  `store_id` smallint(5) unsigned NOT NULL COMMENT 'Store View',\n  `is_active` smallint(6) NOT NULL DEFAULT 0 COMMENT 'Is Active',\n  `environment_type` varchar(12) NOT NULL COMMENT 'Environment Type',\n  `token_expire_time` timestamp NULL DEFAULT NULL COMMENT 'Token Expire Time',\n  `auth_data` text DEFAULT NULL COMMENT 'Authentication Data',\n  `additional_data` text DEFAULT NULL COMMENT 'Additional Data',\n  `unique_identification_no` int(11) NOT NULL COMMENT 'Unique Indentification Number',\n  `synchronization_policy_id` smallint(6) NOT NULL COMMENT 'Synchronization Policy ID',\n  `order_policy_id` smallint(6) NOT NULL COMMENT 'Order Policy ID',\n  `creation_time` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Creation Time',\n  `update_time` timestamp NOT NULL DEFAULT current_timestamp() ON UPDATE current_timestamp() COMMENT 'Modification Time',\n  `marketplace_target_id` smallint(5) unsigned NOT NULL COMMENT 'Marketplace Target Id',\n  PRIMARY KEY (`id`),\n  KEY `CED_MARKETPLACE_ACCOUNT_MARKETPLACE_ID_CED_MARKETPLACES_ID` (`marketplace_id`),\n  CONSTRAINT `CED_MARKETPLACE_ACCOUNT_MARKETPLACE_ID_CED_MARKETPLACES_ID` FOREIGN KEY (`marketplace_id`) REFERENCES `ced_marketplaces` (`id`) ON DELETE CASCADE\n) ENGINE=InnoDB AUTO_INCREMENT=9 DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci COMMENT='Marketplace Accounts Table'"