"CREATE TABLE `ebaymultiaccount_accounts` (\n  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Id',\n  `account_code` varchar(255) NOT NULL DEFAULT '' COMMENT 'Account Code',\n  `account_env` varchar(255) DEFAULT '' COMMENT 'Account Environment',\n  `account_location` varchar(50) DEFAULT '' COMMENT 'Account Location',\n  `account_store` varchar(50) DEFAULT '' COMMENT 'Account Store',\n  `account_status` tinyint(1) DEFAULT NULL COMMENT 'Account Status',\n  `account_token` text DEFAULT NULL COMMENT 'Account Token',\n  `expire_time` datetime DEFAULT '0000-00-00 00:00:00' COMMENT 'Hard Expiration Time',\n  PRIMARY KEY (`id`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci COMMENT='ebaymultiaccount_accounts'"