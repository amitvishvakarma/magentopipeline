"CREATE TABLE `ced_listing_template` (\n  `id` smallint(5) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Id',\n  `account_id` smallint(5) unsigned NOT NULL COMMENT 'Account ID',\n  `marketplace_id` tinyint(3) unsigned NOT NULL COMMENT 'Marketplace Id',\n  `image_policy_id` smallint(5) unsigned NOT NULL COMMENT 'Image Policy ID',\n  `pricing_policy_id` smallint(5) unsigned NOT NULL COMMENT 'Pricing Policy ID',\n  `inventory_policy_id` smallint(5) unsigned NOT NULL COMMENT 'Inventory Policy ID',\n  `status` varchar(32) DEFAULT NULL COMMENT 'Listing Template Status',\n  `attribute_mapping` text NOT NULL COMMENT 'Attribute Mapping',\n  `other_policy_ids` varchar(255) DEFAULT NULL COMMENT 'Other Policy Ids',\n  `marketplace_template_id` varchar(255) DEFAULT NULL COMMENT 'Marketplace Template Id',\n  `is_changed` tinyint(1) NOT NULL DEFAULT 0 COMMENT 'Is Changed',\n  `document_ids` text DEFAULT NULL COMMENT 'Document Ids',\n  `api_payload` longtext DEFAULT NULL COMMENT 'Api Payload',\n  `created_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Created At',\n  `updated_at` timestamp NOT NULL DEFAULT current_timestamp() ON UPDATE current_timestamp() COMMENT 'Updated At',\n  `is_valid` tinyint(1) NOT NULL DEFAULT 0 COMMENT 'Is Valid',\n  `code` varchar(100) NOT NULL COMMENT 'Category Code',\n  `name` varchar(100) NOT NULL COMMENT 'Template Name',\n  `hierarchy` varchar(255) NOT NULL COMMENT 'Category Hierarchy',\n  `query_condition` text DEFAULT NULL COMMENT 'Query Condition',\n  `excluded_product_ids` varchar(255) NOT NULL COMMENT 'Excluded Product Ids',\n  `is_override` tinyint(1) DEFAULT 0 COMMENT 'Is Override',\n  PRIMARY KEY (`id`)\n) ENGINE=InnoDB AUTO_INCREMENT=7 DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci COMMENT='Listing Template'"